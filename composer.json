{
    "name": "typhoon/reflection",
    "description": "Typhoon Reflection",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov.valentin@gmail.com"
        },
        {
            "name": "Typhoon Team",
            "homepage": "https://github.com/orgs/typhoon-php/people"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^4.18 || ^5.0",
        "phpstan/phpdoc-parser": "^1.21",
        "psr/simple-cache": "^3.0",
        "typhoon/change-detector": "^0.4",
        "typhoon/declaration-id": "^0.4",
        "typhoon/type": "^0.4",
        "typhoon/typed-map": "^0.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Typhoon\\Reflection\\": ""
        },
        "files": [
            "Internal/functions.php"
        ]
    },
    "config": {
        "lock": false,
        "vendor-dir": "../../var/Reflection/vendor"
    }
}
